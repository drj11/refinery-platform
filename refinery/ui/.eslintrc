{
  "env": {
    "browser": true
  },

  "globals": {
    "angular": true,
    "module": true,
    "require": true,
    "console": true
  },

  "rules": {
    "array-bracket-spacing": [2, "never"],
    "brace-style": [2, "1tbs"],
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "key-spacing": 0,
    "no-undef": 2,
    "object-curly-spacing": [2, "never"],
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "wrap-iife": 2
  }
}
